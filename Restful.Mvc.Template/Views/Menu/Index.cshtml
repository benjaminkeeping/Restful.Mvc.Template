@using Mvc.Extensions
@using Restful.Wiretypes
@model IEnumerable<GroupOf<Link>>

@if (Model.Count() > 0)
{
    <div class="well sidebar-nav">
        <ul class="nav nav-list">        @foreach (var groupOfLinks in Model)
        {
            <li class="nav-header">@groupOfLinks.GroupName</li>
            foreach (var link in groupOfLinks.Items)
            {
                <li>@Html.Link(link)</li>
            }
        }    </ul></div>
}
else
{
    <div class="sidebar-nav"></div>
}